{
  "name": "ipg-taxonomy-extractor",
  "version": "v2.0.0",
  "description": "Office Add-in for extracting specific segments from pipe-delimited taxonomy data",
  "main": "./dist/src/taskpane/taskpane.js",
  "scripts": {
    "build": "npm run clean && npm run build:addin && npm run build:worker",
    "build:addin": "webpack --mode production --config webpack.config.js",
    "build:worker": "webpack --mode production --config webpack.worker.config.js",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production --analyze",
    "dev-server": "webpack serve --mode development",
    "start": "office-addin-dev-certs verify && webpack serve --mode development --open",
    "start:desktop": "office-addin-dev-certs verify && webpack serve --mode development",
    "start:web": "office-addin-dev-certs verify && webpack serve --mode development --env WEBPACK_BUILD=web",
    "watch": "webpack --mode development --watch",
    "validate": "npx office-addin-manifest validate manifest.xml",
    "stop": "npx office-addin-debugging stop manifest.xml",
    "lint": "echo 'Linting not configured yet'",
    "test": "echo 'Tests not configured yet'",
    "clean": "rimraf dist"
  },
  "keywords": [
    "Office Add-in",
    "Excel",
    "Taxonomy",
    "Data Processing"
  ],
  "author": "Henrik",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "core-js": "^3.36.0",
    "regenerator-runtime": "^0.14.1"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250803.0",
    "@types/office-js": "^1.0.374",
    "canvas": "^3.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "esbuild-loader": "^4.3.0",
    "file-loader": "^6.2.0",
    "glob": "^11.0.3",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.0",
    "office-addin-dev-certs": "^2.0.3",
    "rimraf": "^6.0.1",
    "source-map-loader": "^5.0.0",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.0.2",
    "wrangler": "^4.0.0"
  },
  "browserslist": [
    "ie 11"
  ]
}
